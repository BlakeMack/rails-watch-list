<%# <h1>Lists#index</h1> %>
  <h1>Check Out Your Movie Lists Below!</h1>

  <% @lists.each do |list| %>
      <%= link_to list_path(list) do %>
          <div class="card">
              <%= list.name %>
          </div>
      <% end %>
  <% end %>

<%= link_to "make a new list!", new_list_path  %>


